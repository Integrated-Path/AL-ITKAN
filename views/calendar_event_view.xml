<odoo>
    <data>

        <record id="calendar_event_form_itkan" model="ir.ui.view">
            <field name="name">calendar.event.form.itkan</field>
            <field name="model">calendar.event</field>
            <field name="inherit_id" ref="calendar.view_calendar_event_form"/>
            <field name="arch" type="xml">
                <xpath expr="//h2" position="after">
                    <label for="send_email_to_attendees"/>
                    <field name="send_email_to_attendees"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>